<div>
  <h2>Hello World!</h2>
</div>
<div class="listings">
  {{#each items as |item|}}
    <article class="item-card">
      <div class="card-image">
        <img src="{{image}}" alt="{{title}}" />
      </div>
      <div class="card-header">
        <h2>From {{User.first_name}}</h2>
        <p>{{description}}</p>
      </div>
    </article>
  {{/each}}
</div>